{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console": [
            "off"
        ]
    },
    "globals": {
        "require": false,
        "samplesPerFrame": false,
        "samplesPerBeat": false,
        "samplesPerSecond": false,
        "basisFrequency": false,
        "sin": false,
        "square": false,
        "saw": false,
        "triangle": false,
        "whitenoise": false,
        "sum": false,
        "mix": false,
        "sequence": false,
        "value": false,
        "readFile": false,
        "bpm": false,
        "note": false,
        "nullAudioProcess": false
    }
}
